<resources>
    <string name="app_name">Read it</string>

    <!--For date Formats-->
    <string name="second_ago">$1%d second ago</string>
    <string name="seconds_ago">$1%d seconds ago</string>
    <string name="minute_ago">$1%d minute ago</string>
    <string name="minutes_ago">$1%d minutes ago</string>
    <string name="hour_ago">$1%d hour ago</string>
    <string name="hours_ago">$1%d hours ago</string>
    <string name="yesterday">Yesterday</string>
    <plurals name="seconds_passed">
        <item quantity="one">@string/second_ago</item>
        <item quantity="other">@string/seconds_ago</item>
    </plurals>
    <plurals name="minutes_passed">
        <item quantity="one">@string/minute_ago</item>
        <item quantity="other">@string/minutes_ago</item>
    </plurals>
    <plurals name="hours_passed">
        <item quantity="one">@string/hour_ago</item>
        <item quantity="other">@string/hours_ago</item>
    </plurals>
    <!--Search dynamic module title-->
    <string name="title_search">Search</string>
</resources>